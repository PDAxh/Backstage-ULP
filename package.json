{
  "name": "backstage-ulp",
  "version": "0.1.0",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "build": "backstage-cli repo build",
    "clean": "backstage-cli repo clean",
    "test": "backstage-cli repo test",
    "test:all": "backstage-cli repo test --coverage",
    "lint": "backstage-cli repo lint --since HEAD~1",
    "lint:all": "backstage-cli repo lint",
    "prepack": "backstage-cli repo prepack",
    "postpack": "backstage-cli repo postpack",
    "docker:build": "docker build . -f packages/backend/Dockerfile --tag backstage-ulp",
    "docker:run": "docker run --rm -p 3000:3000 -p 7007:7007 backstage-ulp"
  },
  "devDependencies": {
    "@backstage/cli": "^0.33.1"
  },
  "dependencies": {
    "@backstage/app-defaults": "^1.6.4",
    "@backstage/backend-defaults": "^0.11.1",
    "@backstage/catalog-model": "^1.7.5",
    "@backstage/cli": "^0.33.1",
    "@backstage/core-app-api": "^1.12.3",
    "@backstage/core-components": "^0.18.1",
    "@backstage/core-plugin-api": "^1.8.1",
    "@backstage/integration-react": "^1.2.0",
    "@backstage/plugin-catalog": "^1.18.0",
    "@backstage/plugin-catalog-common": "^0.2.3",
    "@backstage/plugin-catalog-graph": "^0.2.40",
    "@backstage/plugin-catalog-import": "^0.10.2",
    "@backstage/plugin-catalog-react": "^1.9.0",
    "@backstage/plugin-github-actions": "^0.6.10",
    "@backstage/plugin-permission-react": "^0.4.19",
    "@backstage/plugin-scaffolder": "^1.19.0",
    "@backstage/plugin-search": "^1.5.0",
    "@backstage/plugin-search-react": "^1.7.0",
    "@backstage/plugin-tech-radar": "^0.6.10",
    "@backstage/plugin-techdocs": "^1.9.0",
    "@backstage/plugin-techdocs-module-addons-contrib": "^1.1.0",
    "@backstage/plugin-techdocs-react": "^1.1.0",
    "@backstage/plugin-user-settings": "^0.8.0",
    "@backstage/theme": "^0.4.1",
    "@backstage/plugin-kubernetes": "^0.12.0",
    "@backstage/plugin-argo-cd": "^0.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.8.1",
    "react-router-dom": "^6.8.1"
  },
  "resolutions": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1"
  }
}
